{
  "name": "vue-webpack-4",
  "version": "1.1.0",
  "description": "A Vuejs template using Webpack 4",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --progress --config build/webpack.config.dev.js",
    "build_webpack": "webpack --config build/webpack.config.prod.js",
    "build_rollup": "rollup ./src/index.js --file dist_rollup/app.js --format umd --name rollup_test",
    "build_rollup_config": "rollup -c ./build/rollup.config.js",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "test:unit": "jest --config test/jest.config.js",
    "test": "npm run lint && npm run test:unit",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --config test/jest.config.js",
    "styleguidist": "vue-styleguidist server",
    "styleguidist:build": "vue-styleguidist build"
  },
  "keywords": [
    "Vue",
    "Webpack 4"
  ],
  "author": "Daniel Cook",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.6.11",
    "vue-router": "^3.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.8.4",
    "@rollup/plugin-alias": "^3.1.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-image": "^2.0.4",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@rollup/plugin-url": "^4.0.2",
    "@testing-library/vue": "^4.1.0",
    "@vue/test-utils": "^1.0.0-beta.31",
    "autoprefixer": "^9.7.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^7.0.0-beta.3",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^2.5.7",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "eslint-plugin-vue-libs": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jest-serializer-vue": "^2.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "rollup": "^2.7.6",
    "rollup-plugin-babel-minify": "^10.0.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-gen-html": "^0.1.1",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-vue": "^5.1.0",
    "style-loader": "^1.1.3",
    "uglify-es": "^3.3.9",
    "url-loader": "^3.0.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2",
    "vue-styleguidist": "^4.19.5",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
